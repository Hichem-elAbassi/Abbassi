<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Projets" table="projets">
    <id name="projetId" type="integer" column="projet_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="jobownerId" type="integer" column="jobOwner_id" nullable="true"/>
    <field name="titre" type="string" column="titre" length="255" nullable="true"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
    <field name="categorie" type="string" column="categorie" length="255" nullable="true"/>
    <field name="nombreFreelancer" type="integer" column="nombre_freelancer" nullable="true"/>
    <field name="level" type="string" column="level" length="255" nullable="true"/>
    <field name="typecontrat" type="string" column="typeContrat" length="255" nullable="true"/>
    <field name="salaire" type="float" column="salaire" precision="10" scale="0" nullable="true"/>
    <field name="duree" type="string" column="duree" length="255" nullable="true"/>
    <field name="etat" type="boolean" column="etat" nullable="true"/>
  </entity>
</doctrine-mapping>
