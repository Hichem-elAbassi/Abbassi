<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Utilisateurs" table="utilisateurs">
    <id name="utilisateurId" type="integer" column="utilisateur_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="true"/>
    <field name="prenom" type="string" column="prenom" length="255" nullable="true"/>
    <field name="age" type="integer" column="age" nullable="true"/>
    <field name="sexe" type="string" column="sexe" length="255" nullable="true"/>
    <field name="adresse" type="string" column="adresse" length="255" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="true"/>
    <field name="password" type="string" column="password" length="255" nullable="true"/>
    <field name="type" type="string" column="type" length="20" nullable="true"/>
    <field name="photoprofil" type="string" column="photoprofil" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
