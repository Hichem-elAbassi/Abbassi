<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cv" table="cv">
    <id name="cvId" type="integer" column="cv_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="skills" type="string" column="skills" length="255" nullable="false"/>
    <field name="certifications" type="string" column="certifications" length="255" nullable="false"/>
    <field name="education" type="string" column="education" length="255" nullable="false"/>
    <field name="experiences" type="string" column="experiences" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
